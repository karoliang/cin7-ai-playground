{
  "name": "cin7-ai-playground",
  "version": "2.0.0",
  "description": "CIN7 AI-powered multi-page application builder with enhanced contextual updates and modern build system",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:watch & npm run dev:server",
    "dev:watch": "node scripts/dev-watcher.js",
    "dev:server": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:generated": "node scripts/build-generated.js",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "auto-commit": "bash scripts/auto-commit.sh",
    "generate-commit-message": "node scripts/generate-commit-message.js",
    "create-branch": "bash scripts/create-branch.sh",
    "post-build": "npm run docs:generate && npm run auto-commit",
    "docs:generate": "typedoc src --out docs/api",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "pre-push": "npm run test && npm run lint",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "dependencies": {
        "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@shopify/polaris": "^13.9.5",
    "@shopify/polaris-tokens": "^9.4.2",
    "@supabase/supabase-js": "^2.55.0",
    "@types/jszip": "^3.4.0",
    "@uiw/react-codemirror": "^4.25.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.0.4",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "commitizen": "^4.3.0",
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.0",
    "playwright": "^1.40.0",
    "@playwright/test": "^1.40.0",
    "typedoc": "^0.25.0",
    "vitepress": "^1.0.0",
    "prettier": "^3.0.0",
    "chokidar": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karoliang/cin7-ai-playground.git"
  },
  "keywords": [
    "cin7",
    "ai",
    "code-generation",
    "playground",
    "multi-page",
    "polaris",
    "react"
  ],
  "author": "Karoliang",
  "license": "ISC",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test && npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt-cli"
    }
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  }
}
